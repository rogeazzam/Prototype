            <Font name="System Bold Italic" size="17.0" />
         </font>
      </Label>
      <ImageView fx:id="movieImg" fitHeight="365.0" fitWidth="381.0" layoutX="95.0" layoutY="90.0" pickOnBounds="true" preserveRatio="true" />
      <Label layoutX="95.0" layoutY="533.0" text="Available times:" textFill="WHITE">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="movieTimes" alignment="CENTER" contentDisplay="CENTER" layoutX="244.0" layoutY="533.0" text="Label" textAlignment="JUSTIFY" textFill="WHITE">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button layoutX="1106.0" layoutY="65.0" mnemonicParsing="false" onAction="#purchaseBtn" prefHeight="25.0" prefWidth="77.0" text="Purchase" />
      <ChoiceBox fx:id="ticketNum" layoutX="889.0" layoutY="65.0" prefWidth="150.0" />
      <Button layoutX="95.0" layoutY="28.0" mnemonicParsing="false" onAction="#backOP" prefHeight="25.0" prefWidth="66.0" styleClass="fruit-item" stylesheets="@style.css" text="Back" />
